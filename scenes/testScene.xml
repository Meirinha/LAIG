<yas>
	<scene root="rootScene" axis_length="5.0" />
	<views default="perp" >
		<perspective id="perp" near="0.1" far="900" angle="90.0">
			<from x="15" y="15" z="15" />
			<to x="0" y="0" z="0" />
		</perspective>
		<ortho id="orth" near="0.3" far="400" left="-30" right="30" top="30" bottom="-30" >
			<from x="40" y="40" z="40" />
			<to x="0" y="0" z="0" />
		</ortho>
	</views>
	<ambient>
		<ambient r="1.0" g="1.0" b="1.0" a="1.0" />
		<background r="0.0" g="0.0" b="0.0" a="1.0" />
	</ambient>
	<lights>
		<omni id="lightOmni" enabled="1" >
			<location x="1" y="0" z="0" w="0" />
			<ambient r="1" g="1" b="1" a="1" />
			<diffuse r="1" g="1" b="1" a="1" />
			<specular r="1" g="1" b="1" a="1" />
		</omni>
		<spot id="lightSpot" enabled="1" angle="90.0" exponent="90.0">
			<location x="0" y="0" z="1" w="0" />
			<target x="0" y="0.0" z="0.0" />
			<ambient r="0.5" g="0.0" b="0.0" a="0.0" />
			<diffuse r="0.5" g="0.0" b="0.0" a="0.0" />
			<specular r="0.5" g="0.0" b="0.0" a="0.0" />
		</spot>
	</lights>
	<textures>
		<texture id="textureID" file="vidral.jpg" />
		<texture id="textureRocks" file="rocks.jpg" />
		<texture id="textureWall" file="bank.jpg" />
		<texture id="textureFloor" file="trunk-image.jpg" />
	</textures>
	<materials>
		<material id="materialID" shininess = "1.0" >
			<emission r="0.5" g="0.0" b="0.0" a="1.0" />
			<ambient r="0.5" g="0.0" b="0.0" a="1.0" />
			<diffuse r="0.5" g="0.0" b="0.0" a="1.0" />
			<specular r="0.5" g="0.0" b="0.0" a="1.0" />
		</material>
		<material id="materialID2" shininess = "1.0" >
			<emission r="0.5" g="1.0" b="1.0" a="1.0" />
			<ambient r="0.5" g="1.0" b="1.0" a="1.0" />
			<diffuse r="0.5" g="1.0" b="1.0" a="1.0" />
			<specular r="0.5" g="1.0" b="0.0" a="1.0" />
		</material>
	</materials>
	<transformations>
		<transformation id="transID">
			<scale x="1.0" y="1.0" z="1.0" />
		</transformation>
		<transformation id="wall1">
			<translate x="0.0" y="0.0" z="10" />
			<rotate axis="y" angle="90.0" />
		</transformation>
		<transformation id="floor">
			<translate x="10.0" y="0.0" z="10" />
			<rotate axis="z" angle="90.0" />
			<rotate axis="y" angle="90.0" />
		</transformation>
		<transformation id="cube">
			<translate x="4" y="0.0" z="4" />
		</transformation>
		<transformation id="quad1">
			<translate x="1.5" y="0.0" z="1.5" />
			<rotate axis="y" angle="90.0" />
		</transformation>
		<transformation id="quad2">
			<translate x="0.0" y="0.0" z="1.5" />
		</transformation>
		<transformation id="quad3">
			<rotate axis="y" angle="-90.0" />
		</transformation>
		<transformation id="quad4">
			<translate x="0.0" y="2.0" z="1.5" />
			<rotate axis="x" angle="-90.0" />
		</transformation>
		<transformation id="quad5">
			<translate x="1.5" y="0.0" z="0.0" />
			<rotate axis="y" angle="-180.0" />
		</transformation>
		<transformation id="dome">
			<rotate axis="x" angle="-90.0" />
			<scale x="80.0" y="80.0" z="80.0" />
		</transformation>
		<transformation id="domebot">
			<rotate axis="x" angle="90.0" />
			<scale x="80.0" y="80.0" z="80.0" />
		</transformation>
		<transformation id="torus">
			<scale x="1.0" y="0.3" z="1.0" />
			<rotate axis="x" angle="90.0" />
		</transformation>
		<transformation id="lake">
			<scale x="62.0" y="60.0" z="62.0" />
			<rotate axis="x" angle="90.0" />
		</transformation>
		<transformation id="circle">
			<translate x="0.0" y="-5.0" z="0.0" />
			<scale x="60.0" y="1.0" z="60.0" />
			<rotate axis="x" angle="-90.0" />
		</transformation>
		<transformation id="island">
			<translate x="0.0" y="0.0" z="0.0" />
			<rotate axis="x" angle="90.0" />
		</transformation>
	</transformations>
	<primitives>
		<primitive id="rec">
			<rectangle x1="10.0" y1="5.0" x2="0.0" y2="0.0" />
		</primitive >
		<primitive id="rec1">
			<rectangle x1="10.0" y1="10.0" x2="0.0" y2="0.0" />
		</primitive >
		<primitive id="quad">
			<rectangle x1="1.5" y1="2.0" x2="0.0" y2="0.0" />
		</primitive >
		<primitive id="quadtop">
			<rectangle x1="1.5" y1="1.5.0" x2="0.0" y2="0.0" />
		</primitive >
		<primitive id="tri">
			<triangle x1="0.0" y1="0.0" z1="0.0" x2="0.0" y2="2.0" z2="0.0" x3="3.0" y3="0.0" z3="0.0"/>
		</primitive >
		<primitive id="cyl">
			<cylinder base="30.0" top="0.0" height="10" slices="10" stacks="5"/>
		</primitive>
		<primitive id="torus">
			<torus inner="60.0" outer="80.0" slices="80" loops="80"/>
		</primitive>
		<primitive id="Lake">
			<Dome slices="50" stacks="50"/>
		</primitive>
		<primitive id="Dome">
			<Dome slices="50" stacks="50"/>
		</primitive>
		<primitive id="DomeBot">
			<SemiSphere slices="50" stacks="50"/>
		</primitive>
		<primitive id="Circle">
			<Circle slices="50"/>
		</primitive>
	</primitives >
	<components>
		<component id="wall">
			<transformation>
				<transformationref id="wall1" />
			</transformation>
			<materials>
				<material id="inherit" />
			</materials>
			<texture id="textureWall" length_s="1.0" length_t="1.0" />
